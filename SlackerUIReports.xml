<Ui xmlns="http://www.blizzard.com/wow/ui/">
	<Frame name="SlackerUI_ReportTemplate" parent="UIParent" toplevel="true" hidden="true" enableMouse="true" movable="true" virtual="true">
		<Scripts>
			<OnLoad>self:RegisterForDrag("LeftButton");</OnLoad>
			<OnDragStart>self:StartMoving();</OnDragStart>
			<OnDragStop>self:StopMovingOrSizing();</OnDragStop>
        </Scripts>
		<Size>
			<AbsDimension x="1024" y="640" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Title" text="Title" inherits="GameFontHighlight"  >
					<Anchors>
						<Anchor point="TOP" x="0" y="-12" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
		</Backdrop>
		<Frames>
			<Button inherits="UIPanelCloseButton">
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset x="-4" y="-4"/>
					</Anchor> 
				</Anchors>
				<Scripts>
					<OnClick> self:GetParent():Hide(); </OnClick>
				</Scripts>
			</Button>
			<Button parentKey="Button1" inherits="UIPanelButtonTemplate" text="Button1">
				<Anchors>
					<Anchor point="TOPLeft">
						<Offset x="12" y="-12"/>
					</Anchor> 
				</Anchors>
				<Size>
					<AbsDimension x="64" y="16" />
				</Size>
				<Scripts>
				</Scripts>
			</Button>
			<Button parentKey="Button2" inherits="UIPanelButtonTemplate" text="Button2">
				<Anchors>
					<Anchor point="TOPLeft">
						<Offset x="96" y="-12"/>
					</Anchor> 
				</Anchors>
				<Size>
					<AbsDimension x="64" y="16" />
				</Size>
				<Scripts>
				</Scripts>
			</Button>
			<Frame parentKey="Table">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="16" y="-38"/>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="16" y="8"/>
					<Anchor point="RIGHT" relativePoint="RIGHT" x="-16"/>
				</Anchors>
			</Frame>
		</Frames>
	</Frame>
</Ui>